  "description": "Multi device whatsapp bot.",
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=21 <25"
  },
  "scripts": {
    "run": "pm2 start . --deep-monitoring --attach --name NEX || node .",
    "asta": "pm2 start . --deep-monitoring --attach --name NEX || node .",
    "start": "pm2 start . --attach --name NEX",
    "stop": "pm2 stop NEX",
    "heroku-postbuild": "git clone https://github.com/terrizevdev/NEX temp && mv temp/.git .git && rm -rf temp && npx puppeteer@latest install --y",
    "fis-heroku-postbuild": "git clone https://github.com/terrizevdev/NEX.git temp1 && rsync -av temp1/* ./ && rsync -av temp1/.* ./ && rm -rf temp1 && npm i && npx puppeteer@latest install",
    "postinstall": "npx puppeteer@latest install --y"
  },
